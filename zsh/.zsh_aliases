# Other Aliases
alias choose='ruby -e "puts ARGV.sample"'
alias tmux='env TERM=xterm-256color tmux'
alias ctags='`brew --prefix`/bin/ctags'
alias brake='bundle exec bin/rake'
alias gcad='git checkout apollo-dev'
alias fms='cf && nvim .'
alias cf='cd ~/code/fms'
alias cs='cd ~/code/sesc'
alias cl='cd ~/code/lazarus_api'
alias fuck='sudo $(fc -ln -1)'
alias ts='tig status'
alias tb='tig blame'
alias nv= 'nvim .'
alias nvv='nvim ~/.config/nvim/init.vim'
alias vv='vim ~/.vimrc'
alias vt='vim ~/.vimtricks'
alias vk='vim ~/.kwm/kwmrc'
alias sv='source ~/.vimrc'
alias svi='sudo vim -u ~/.vimrc'
alias clock='tty-clock -tc -C 6'
alias vz='nvim ~/.zshrc'
alias vza='nvim ~/.zsh_aliases'
alias sz='source ~/.zshrc'
alias ...='../..'
alias ....='../../..'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias tree='tree --charset iso-8859'
alias rc='rails c'
alias be='bundle exec'
alias btr='bash ~/misc/bt_release.sh'
alias bts='bash ~/misc/bt_steal.sh'
