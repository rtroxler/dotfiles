# Other Aliases
alias jshint='~/.npm/jshint/1.0.0/package/bin/jshint'
alias fms='cf && vim .'
alias choose='ruby -e "puts ARGV.sample"'
alias xmod='xmodmap ~/.Xmodmap'
alias xmodquerty='xmodmap. ~/.Xmodmap-querty'
alias tmux='tmux -2'
alias work='tmux -2 a -t work'
alias cf='cd ~/code/fms'
alias vi='vim'
alias vim='vim --servername DEV'
alias vx='vi ~/.Xresources'
alias sx='xrdb ~/.Xresources'
alias fuck='sudo $(fc -ln -1)'
alias ts='tig status'
alias tb='tig blame'
alias vv='vi ~/.vimrc'
alias vt='vi ~/.vimtricks'
alias sv='source ~/.vimrc'
alias svi='sudo vi -u ~/.vimrc'
alias vbox='vboxheadless -s devvm1.3 &'
alias clock='tty-clock -tc -C 6'
alias fuckcaps='xmodmap -e "clear Lock"; setxkbmap -option caps:backspace'
alias vh='vi ~/.config/herbstluftwm/autostart'
alias hc='herbstclient '
alias e='emacs -nw'
alias ez='e ~/.zshrc'
alias vz='vi ~/.zshrc'
alias vza='vi ~/.zsh_aliases'
alias sz='source ~/.zshrc'
alias ...='../..'
alias ....='../../..'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias tree='tree --charset iso-8859'
alias rc='rails c'
